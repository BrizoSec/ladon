# Collection Service Environment Variables

# AlienVault OTX API Key
# Get from: https://otx.alienvault.com/api
ALIENVAULT_API_KEY=your_alienvault_api_key_here

# abuse.ch Auth-Key (free)
# Get from: https://auth.abuse.ch/
# Works for ThreatFox, URLhaus, and MalwareBazaar
ABUSECH_AUTH_KEY=your_abusech_auth_key_here

# Other API Keys (for future use)
MISP_API_KEY=your_misp_api_key_here
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Service Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO

# Pub/Sub Configuration
PUBSUB_PROJECT_ID=ladon-dev
PUBSUB_RAW_IOC_EVENTS_TOPIC=raw-ioc-events
PUBSUB_RAW_ACTIVITY_EVENTS_TOPIC=raw-activity-events

# Storage Service
STORAGE_SERVICE_URL=http://localhost:8000

# For local testing with Pub/Sub emulator
# PUBSUB_EMULATOR_HOST=localhost:8085
